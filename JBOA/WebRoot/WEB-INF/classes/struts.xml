<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE struts PUBLIC
    "-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
    "http://struts.apache.org/dtds/struts-2.0.dtd">

<struts>
	
    <package name="default" namespace="/*" extends="struts-default">
	
		<action name="SysEmpAction" class="cn.action.SysEmpAction">
			<result>index.jsp</result>
			<result name="input">login.jsp</result>
		</action>
		
		<action name="AddClaimVoucherAction" class="cn.action.AddClaimVoucherAction">
			<result>jsp/claim/claim_voucher_update.jsp</result>
			<result name="input">error.jsp</result>
		</action>
		
		<action name="SearchClaimVoucherAction" class="cn.action.SearchClaimVoucherAction">
			<result>jsp/claim/claim_voucher_list.jsp</result>
			<result name="input">welcome.html</result>
		</action>
		
		<action name="ClaimViewAction" class="cn.action.ClaimViewAction">
			<result>jsp/claim/claim_voucher_view.jsp</result>
			<result name="input">/error.jsp</result>
		</action>
		
    </package>
	
</struts>
